<launch>
  <node name="pcl_test_node" pkg="swanson_algorithms" type="pcl_tests" output="screen">
    <rosparam>
      cloud_topic: "vboats/cloud/raw"
      world_frame: "base_link"
      camera_frame: "camera_depth_optical_frame"
      voxel_leaf_size: 0.001 <!-- mm -->
      x_filter_min: -10.0 <!-- m -->
      x_filter_max: 10.0 <!-- m -->
      y_filter_min: -10.0 <!-- m -->
      y_filter_max: 10.0 <!-- m -->
      z_filter_min: -1.1 <!-- m -->
      z_filter_max: 1.1 <!-- m -->
      plane_max_iterations: 100
      plane_distance_threshold: 0.05
      cluster_tolerance: 0.01
      cluster_min_size: 100
      cluster_max_size: 500000
    </rosparam>
  </node>
</launch>
